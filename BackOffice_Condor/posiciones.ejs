    <%- include('base.ejs') %>
    <div class="contenido">
      <div id="tabs" style="width: 99%; margin-left: auto; margin-right: auto; height: 96%">
        <ul>
          <li><a href="#USA-IBKR">Posiciones USA y IBKR</a></li>
          <li><a href="#ARG-LATIN">Posiciones Argentina y Latin</a></li>
          <li><a href="#TRANSACTIONS-CONVERS">Transactions y Conversiones</a></li>
          <li><a href="#SECURETYLIST">Securyti list</a></li>
        </ul>
          <div id="infoStatus"></div>
          <div id="USA-IBKR">
            <span>
              <button id="load-date-usa-ibkr" class="ui-button ui-widget ui-corner-all">Cargar datos</button>
            </span>
            <table id="tableDiferencias-USA-IBKR"  style="width: 100% !important; table-layout: fixed;" class="row-border hover order-column compact cell-border nowrap">
                <thead></thead>
                <tbody></tbody>
            </table>
            <div id="divModalUsaIbkr">
              <form id="modalUsaIbkr">
              <table style="padding: 10px 16px 0px 10px;">
                <tbody>
                  <tr>
                    <td align="right">
                      <label>Posiciones condor&nbsp;:&nbsp;</label>
                    </td>
                    <td align="left" id="tdUsa">
                      <div id="drop-zone-usa" class="drop-zone">
                        Haga click o arrastre el archivo aqui
                      </div>
                    </td>
                    <td>
                      <button type="button" id="deleteUsa" class="ui-button" style="width: 24px; height: 24px; padding: 0;" data-id="${uniqueId}"> 
                        <i class="fa fa-trash" style="font-size: 14px;"></i>
                      </button> 
                    </td>
                  </tr>
                  <tr>
                    <td align="right">
                      <label>Posiciones IBKR&nbsp;:&nbsp;</label>
                    </td>
                    <td align="left" id="tdIbkr">
                      <div id="drop-zone-ibkr" class="drop-zone">
                        Haga click o arrastre el archivo aqui
                      </div>
                    </td>
                    <td>
                      <button type="button" id="deleteIbkr" class="ui-button" style="width: 24px; height: 24px; padding: 0;" data-id="${uniqueId}"> 
                        <i class="fa fa-trash" style="font-size: 14px;"></i>
                      </button> 
                    </td>
                  </tr>
                </tbody>
              </table>
            </form>
            </div>
         </div>
         <div id="ARG-LATIN">
