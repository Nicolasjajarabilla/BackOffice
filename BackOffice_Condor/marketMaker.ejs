  <body>
    <%- include('base.ejs') %>
    <div class="contenido">
      <div id="makerMakerContent">
        <span>
          <button id="load-date" class="ui-button ui-widget ui-corner-all">
            Cargar datos
          </button>
        </span>
        <table
          id="marketMakerTable"
          style="width: 100% !important; table-layout: fixed"
          class="row-border hover order-column compact cell-border nowrap"
        >
          <thead></thead>
          <tbody></tbody>
        </table>
        <div id="formDate" style="padding: 0px 10px">
          <form id="formValidate">
            <table style="padding: 10px 16px 0px 16px">
              <tr>
                <td align="right">
                  <label for="date">Fecha&nbsp;:&nbsp;</label>
                </td>
                <td align="left">
                  <input
                    style="width: 274px"
                    type="date"
                    id="dateReport"
                    name="date"
                    required
                  />
                </td>
              </tr>
            </table>
          </form>
        </div>
        <div id="modalImage">
          <div id="modalImageContentApertura"></div>
          <div id="modalImageContentRegular"></div>
        </div>
        <div
          id="infoStatus"
          style="
            display: flex;
            align-items: center;
            margin-top: 0.5rem;
            margin-right: 1rem;
            flex-direction: column;
            text-align: center;
          "
        ></div>
        <div id="formDateLoad">
          <form id="formValidateLoad">
            <table>
              <tr>
                <td align="right">
                  <label for="date">Fecha&nbsp;:&nbsp;</label>
                </td>
                <td align="left">
                  <input
                    type="date"
                    style="width: 274px"
                    id="dateReportLoad"
                    name="date"
                    required
                  />
                </td>
              </tr>
            </table>
          </form>
        </div>
      </div>
    </div>
  </body>
